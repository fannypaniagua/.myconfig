[ezoom]
s0_zoom_in_key = <Shift><Super>plus
s0_zoom_out_key = <Shift><Super>underscore
s0_hide_original_mouse = false

[move]
s0_initiate_key = <Alt><Super>Up

[staticswitcher]
s0_next_key = <Control><Primary>Tab
s0_prev_key = <Shift><Control><Primary>Tab
s0_next_all_key = Disabled
s0_prev_all_key = Disabled
s0_next_group_key = <Control><Primary>grave
s0_prev_group_key = <Shift><Control><Primary>asciitilde
s0_popup_delay = 0.000000
s0_mipmap = true
s0_focus_on_switch = true
s0_bring_to_front = true
s0_highlight_mode = 2

[resize]
s0_initiate_key = <Alt><Super>Down
s0_border_color = #0000009f
s0_fill_color = #00000019

[core]
s0_active_plugins = core;composite;opengl;compiztoolbox;decor;imgpng;gnomecompat;place;snap;move;regex;mousepoll;resize;commands;vpswitch;session;animation;wall;workarounds;expo;ezoom;staticswitcher;fade;scale;unityshell;
s0_close_window_key = <Shift><Control><Primary>w
s0_minimize_window_key = <Control><Primary>m
s0_toggle_window_maximized_key = <Shift><Control><Primary>m

[unityshell]
s0_launcher_hide_mode = 1
s0_show_launcher = <Super>space
s0_alt_tab_prev = Disabled
s0_alt_tab_forward_all = Disabled
s0_alt_tab_prev_all = Disabled
s0_icon_size = 32
s0_edge_responsiveness = 8.000000
s0_num_launchers = 1

[decor]
s0_command = gtk-window-decorator --replace

[gnomecompat]
s0_main_menu_key = Disabled
s0_run_key = Disabled

[switcher]
s0_next_key = <Control><Primary>grave
s0_mipmap = false

[grid]
s0_put_left_key = <Alt><Super>Left
s0_put_right_key = <Alt><Super>Right
s0_put_top_key = Disabled
s0_put_bottom_key = Disabled
s0_put_topleft_key = <Alt><Super>Home
s0_put_topright_key = <Alt><Super>Prior
s0_put_bottomleft_key = <Alt><Super>Next
s0_put_bottomright_key = <Alt><Super>End
s0_put_maximize_key = <Shift><Control><Primary>m
s0_put_restore_key = <Shift><Control><Primary><Alt>m
s0_top_left_corner_action = 0
s0_top_edge_action = 0
s0_top_right_corner_action = 0
s0_left_edge_action = 0
s0_right_edge_action = 0
s0_bottom_left_corner_action = 0
s0_bottom_right_corner_action = 0

